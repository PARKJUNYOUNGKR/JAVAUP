package univ;
import java.util.*;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int n1 = sc.nextInt();
		int n2 = n + n1;
		System.out.println("You entered " + n2);
		
		System.out.println("You entered(1) " + (n + n1));
		
		sc.close();
	}

}
