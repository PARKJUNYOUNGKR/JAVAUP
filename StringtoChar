import java.util.*;
public class Main2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine(); //String값을 받아준다.
		char[] s1 = s.toCharArray(); //String값을 Char값으로 바꾸어 준다. -> Char로써 하나하나의 숫자를 받아주기 위함
		int sum = 0;
		for(int i=0; i<s.length(); i++) { //String의 값 받아준다.
			
			sum = sum + (s1[i] - '0'); 
		}
		System.out.println(sum);
		
		}

}
