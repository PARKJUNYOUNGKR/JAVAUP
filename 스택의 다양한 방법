package univ;
import java.util.Stack;
public class Main92 {

	public static void main(String[] args) {
		StringBuffer s = new StringBuffer();
		
		//1
		s.append("1\n");
		s.append("2\n");
		s.append("3\n");
		
		//2
		String a = "1\n";
		 a += "2\n";
		 a += "3\n";
				
		
		System.out.println(s); //1 출력
		System.out.println(a); //2 출력
		
		
	}

}
